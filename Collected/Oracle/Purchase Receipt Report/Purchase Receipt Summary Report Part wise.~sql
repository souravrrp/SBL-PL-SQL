select t.contract,
    t.part_no,
    t.description,
    sum(t.qty_arrived) qty_arrived,
    t.state
from PURCHASE_RECEIPT_NEW t
where t.arrival_date between to_date('&fromDate', 'YYYY/MM/DD') and to_date('&toDate', 'YYYY/MM/DD') and
  t.state = 'Received' and
  t.contract = 'MS2C' 
group by t.contract, t.part_no, T.description, t.state
order by t.part_no
